package br.com.unip.garage.controle;

import org.cocos2d.layers.CCLayer;
import org.cocos2d.nodes.CCSprite;

public class Botao extends CCLayer {
	private CCSprite imagemBotao;
	private ButtonDelegate delegate;

	public Botao(String buttonImage) {
		this.setIsTouchEnabled(true);
		this.imagemBotao = CCSprite.sprite(buttonImage);
		addChild(this.imagemBotao);
	}

}
